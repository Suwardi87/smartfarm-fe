{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "isolatedModules": true,
    "noEmit": true,
    "ignoreDeprecations": "6.0", 
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
   "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"],
}
